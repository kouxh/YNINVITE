.pages-contact-index{
  width: 100%;
  height: 100vh;
}
.list-box{
  margin-top: 20rpx;
}

.each-list{
  width: 604rpx;
	height: 112rpx;
	background-color: #fff;
	box-shadow: 0rpx 4rpx 12rpx 0rpx 
		#f2f2f2;
  border-radius: 10rpx;
  padding: 25rpx 40rpx 20rpx 32rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  margin-bottom: 24rpx;
}
.list-l{
  width: 80%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.list-top{
  font-size: 28rpx;
  color: #333333;
  font-weight: 600;
}
.list-base{
  display: flex;
  align-items: center;
}

.person{
  font-size: 28rpx;
	letter-spacing: 0rpx;
  color: #999999;
  margin-left: 10rpx;
}
.icon-iconfontlianxiren01{
  font-size: 24rpx;
}
.list-r {
  font-size: 28rpx;
  color: #333333;
  display: flex;
  align-items: center;
}
.common{
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
}
.success{
	background-color: #1784e5;
}

.no-check{
  background-color: #b3b3b3;
}

.text{
  letter-spacing: 1rpx;
  padding-left: 16rpx;
}
.creatActivityFn{
  width: 676rpx;
	height: 82rpx;
	background-image: linear-gradient(-75deg, 
		rgba(0, 77, 204, 1) 0%, 
		#46c0ff 100%), 
	linear-gradient(
		#eeeeee, 
		#eeeeee);
	background-blend-mode: normal, 
		normal;
	box-shadow: 0rpx 4rpx 10rpx 0rpx 
		#c6c6c6;
  border-radius: 10rpx;
  margin: 0 auto;
  font-size: 32rpx;
  color: #ffffff;
  text-align: center;
  line-height: 82rpx;
  margin-top: 70rpx;
}

