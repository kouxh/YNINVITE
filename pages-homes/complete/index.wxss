@import "/pages-homes/nominate/index.wxss";

.pages-complete-index {
  width: 100%;
  height: 100vh;
}

.special {
  width: 44% !important;
}
.form-content {
  margin-bottom: 200rpx;
}

.radio-box{
  display: flex;
  align-items: center;
  margin-right: 60rpx;
}
.checked{
  width: 18rpx;
  height: 18rpx;
  border-radius: 50%;
	border: solid 1rpx #999999;
}
.checked-text{
  font-size: 24rpx;
  color: #333333;
  margin-left: 10rpx;
}
.active{
  border: none;
  background-color: #1784e5;
}
.radio-text{
  font-size: 34rpx;
  letter-spacing: 0rpx;
  color: #333333;
}