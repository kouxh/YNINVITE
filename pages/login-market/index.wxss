/* pages/login-market/index.wxss */.pages-login-index {
  width: 100vw;
  height: 100vh;
  /* background-image: url("https://march.yuanian.com/static/assets/img/login3.png");
  background-size: 100% 100vh;
  background-repeat: no-repeat; */
}
.login-content{
  padding-top: 166rpx;
  position: relative;
  z-index: 99;
}
.logo-box {
  width: 148rpx;
  height: 114rpx;
  margin: 0 auto;
}

.title {
  font-size: 46rpx;
  letter-spacing: 6rpx;
  color: #333333;
  text-align: center;
  margin-top: 136rpx;
}

.btn-box {
  margin-top: 180rpx;
}

.btn {
  width: 334rpx;
  height: 70rpx;
  background-image: linear-gradient(-75deg,
      rgba(0, 77, 204, 1) 0%,
      #46c0ff 100%),
    linear-gradient(#003792,
      #003792);
  background-blend-mode: normal,
    normal;
  box-shadow: 0rpx 0rpx 13rpx 0rpx #d4d4d4;
  border-radius: 9rpx;
  margin: 0 auto;
  font-size: 28rpx;
  color: #fff;
  letter-spacing: 2rpx;
  text-align: center;
  line-height: 70rpx;
  margin-bottom: 44rpx;
}
.left{
  width: 276rpx;
  height: 276rpx;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
}
.right{
  width: 460rpx;
	height: 580rpx;
  position: fixed;
  right: 0rpx;
  bottom: 0rpx;
  z-index: 1;
}
